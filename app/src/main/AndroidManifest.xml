<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.com.rahmandev" >

    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:name="com.com.rahmandev.app.AppController"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.com.rahmandev.activity.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.com.rahmandev.activity.MyBottleActivity"
            android:label="Event" />
        <activity
            android:name="com.com.rahmandev.activity.LoginActivity"
            android:label="Login" />
        <activity
            android:name="com.com.rahmandev.activity.RegisterActivity"
            android:label="Register" />
        <activity
            android:name="com.com.rahmandev.activity.BarcodeScanner"
            android:label="QR Code Scanner" />
        <activity
            android:name="com.com.rahmandev.textdrawable.TextDrawableActivity"
            android:label="List TextDrawable" />
        <activity
            android:name="com.com.rahmandev.textdrawable.ListActivity"
            android:label="List TextDrawable" />
    </application>

</manifest>